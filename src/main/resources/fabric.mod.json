{
  "schemaVersion": 1,
  "id": "createutilities",
  "version": "${version}",
  "name": "Create Utilities",
  "description": "Adds extra utilities to the Create Mod",
  "authors": [
    "Duquee_"
  ],
  "contact": {
    "sources": "https://github.com/Duqueeee/create-utilities"
  },
  "license": "CC0",
  "icon": "assets/createutilities/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "me.duquee.createutilities.CreateUtilities"
    ],
    "client": [
      "me.duquee.createutilities.CreateUtilitiesClient"
    ]
  },
  "mixins": [
    "createutilities.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  },
  "accessWidener" : "createutilities.accesswidener"
}
